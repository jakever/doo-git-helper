{
  "name": "doo-git-helper",
  "version": "1.0.0",
  "description": "GitLab 版本管理工具",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:pack": "npm run build && electron-builder --dir"
  },
  "keywords": [
    "git",
    "gitlab",
    "cherry-pick",
    "electron",
    "vue"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "concurrently": "^8.2.2",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.27",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "axios": "^1.6.0",
    "element-plus": "^2.4.0",
    "pinia": "^2.1.0",
    "simple-git": "^3.19.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  }
}
